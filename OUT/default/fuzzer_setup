# environment variables:
AFL_CUSTOM_INFO_PROGRAM=./jsoncpp_fuzzer
AFL_CUSTOM_INFO_PROGRAM_ARGV=
AFL_CUSTOM_INFO_OUT=OUT/default
AFL_I_DONT_CARE_ABOUT_MISSING_CRASHES=1
AFL_PYTHON_MODULE=main
AFL_SKIP_CPUFREQ=1
# command line:
'afl-fuzz' '-t' '200' '-i' 'IN' '-o' 'OUT' '-x' 'dictionaries/json.dict' './jsoncpp_fuzzer'
